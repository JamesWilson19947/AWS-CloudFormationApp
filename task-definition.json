{
  "family": "custom",
  "executionRoleArn": "arn:aws:iam::511353949006:role/ecsTaskExecutionRole",
  "containerDefinitions": [
      {
          "name": "custom",
          "image": "511353949006.dkr.ecr.eu-west-1.amazonaws.com/docker-php",
          "cpu": 256,
          "portMappings": [
            {
              "hostPort": 80,
              "protocol": "tcp",
              "containerPort": 80
            }
          ],
          "memory": 512,
          "essential": true,
          "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
              "awslogs-group": "/ecs/custom",
              "awslogs-region": "eu-west-1",
              "awslogs-stream-prefix": "ecs"
            }
          }
      }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512"
}